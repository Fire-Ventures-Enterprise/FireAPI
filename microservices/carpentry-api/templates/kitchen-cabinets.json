{
  "job_id": "kitchen_cabinets_carpentry",
  "trade": "carpentry",
  "job_name": "Kitchen Cabinet Installation",
  "description": "Professional kitchen cabinet installation including uppers, lowers, and trim work",
  "specialty": "cabinetry",
  
  "phases": [
    {
      "phase_id": "demolition",
      "phase_name": "Cabinet Demolition",
      "sequence": 1,
      "tasks": [
        {
          "task": "Remove existing upper cabinets",
          "labor_hours_per_lf": 0.3,
          "skill_level": "carpenter",
          "tools": ["pry_bar", "drill", "reciprocating_saw"]
        },
        {
          "task": "Remove existing lower cabinets", 
          "labor_hours_per_lf": 0.4,
          "skill_level": "carpenter",
          "tools": ["pry_bar", "drill", "hand_tools"]
        },
        {
          "task": "Clean and prep installation area",
          "labor_hours_per_lf": 0.1,
          "skill_level": "helper",
          "tools": ["broom", "vacuum", "cleaning_supplies"]
        }
      ]
    },
    {
      "phase_id": "installation",
      "phase_name": "Cabinet Installation", 
      "sequence": 2,
      "tasks": [
        {
          "task": "Install upper cabinets",
          "labor_hours_per_lf": 0.8,
          "skill_level": "carpenter",
          "tools": ["level", "drill", "cabinet_screws", "shims"],
          "notes": "Must be installed first for proper alignment"
        },
        {
          "task": "Install lower cabinets",
          "labor_hours_per_lf": 1.0,
          "skill_level": "carpenter", 
          "tools": ["level", "drill", "cabinet_screws", "shims"],
          "notes": "Level and align with uppers"
        },
        {
          "task": "Install cabinet doors and drawers",
          "labor_hours_per_lf": 0.4,
          "skill_level": "carpenter",
          "tools": ["drill", "hinges", "drawer_slides"]
        },
        {
          "task": "Install cabinet hardware",
          "labor_hours_per_lf": 0.3,
          "skill_level": "carpenter",
          "tools": ["drill", "templates", "handles_knobs"]
        }
      ]
    },
    {
      "phase_id": "finishing",
      "phase_name": "Cabinet Finishing",
      "sequence": 3,
      "tasks": [
        {
          "task": "Install crown molding",
          "labor_hours_per_lf": 0.4,
          "skill_level": "carpenter",
          "tools": ["miter_saw", "finish_nailer", "coping_saw"],
          "condition": "if_crown_molding_specified"
        },
        {
          "task": "Install toe kicks and fillers",
          "labor_hours_per_lf": 0.2,
          "skill_level": "carpenter",
          "tools": ["table_saw", "finish_nailer"]
        },
        {
          "task": "Final adjustments and touch-ups",
          "labor_hours_per_lf": 0.2,
          "skill_level": "carpenter", 
          "tools": ["screwdrivers", "sandpaper", "wood_filler"]
        }
      ]
    }
  ],

  "materials": [
    {
      "category": "cabinet_boxes",
      "items": [
