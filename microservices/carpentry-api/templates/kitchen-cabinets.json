{
  "job_id": "kitchen_cabinets_carpentry",
  "trade": "carpentry",
  "job_name": "Kitchen Cabinet Installation",
  "description": "Professional kitchen cabinet installation including uppers, lowers, and trim work",
  "specialty": "cabinetry",
  
  "phases": [
    {
      "phase_id": "demolition",
      "phase_name": "Cabinet Demolition",
      "sequence": 1,
      "tasks": [
        {
          "task": "Remove existing upper cabinets",
          "labor_hours_per_lf": 0.3,
          "skill_level": "carpenter",
          "tools": ["pry_bar", "drill", "reciprocating_saw"]
        },
        {
          "task": "Remove existing lower cabinets", 
          "labor_hours_per_lf": 0.4,
          "skill_level": "carpenter",
          "tools": ["pry_bar", "drill", "hand_tools"]
        },
        {
          "task": "Clean and prep installation area",
          "labor_hours_per_lf": 0.1,
          "skill_level": "helper",
          "tools": ["broom", "vacuum", "cleaning_supplies"]
        }
      ]
    },
    {
      "phase_id": "installation",
      "phase_name": "Cabinet Installation", 
      "sequence": 2,
      "tasks": [
        {
          "task": "Install upper cabinets",
          "labor_hours_per_lf": 0.8,
          "skill_level": "carpenter",
          "tools": ["level", "drill", "cabinet_screws", "shims"],
          "notes": "Must be installed first for proper alignment"
        },
        {
          "task": "Install lower cabinets",
          "labor_hours_per_lf": 1.0,
          "skill_level": "carpenter", 
          "tools": ["level", "drill", "cabinet_screws", "shims"],
          "notes": "Level and align with uppers"
        },
        {
          "task": "Install cabinet doors and drawers",
          "labor_hours_per_lf": 0.4,
          "skill_level": "carpenter",
          "tools": ["drill", "hinges", "drawer_slides"]
        },
        {
          "task": "Install cabinet hardware",
          "labor_hours_per_lf": 0.3,
          "skill_level": "carpenter",
          "tools": ["drill", "templates", "handles_knobs"]
        }
      ]
    },
    {
      "phase_id": "finishing",
      "phase_name": "Cabinet Finishing",
      "sequence": 3,
      "tasks": [
        {
          "task": "Install crown molding",
          "labor_hours_per_lf": 0.4,
          "skill_level": "carpenter",
          "tools": ["miter_saw", "finish_nailer", "coping_saw"],
          "condition": "if_crown_molding_specified"
        },
        {
          "task": "Install toe kicks and fillers",
          "labor_hours_per_lf": 0.2,
          "skill_level": "carpenter",
          "tools": ["table_saw", "finish_nailer"]
        },
        {
          "task": "Final adjustments and touch-ups",
          "labor_hours_per_lf": 0.2,
          "skill_level": "carpenter", 
          "tools": ["screwdrivers", "sandpaper", "wood_filler"]
        }
      ]
    }
  ],

  "materials": [
    {
      "category": "cabinet_boxes",
      "items": [
        {
          "item": "Upper Cabinet Boxes",
          "unit": "linear_foot",
          "category": "cabinetry",
          "specification": "Standard upper cabinet boxes with adjustable shelves",
          "calculation": "upper_cabinet_linear_feet",
          "waste_factor": 0.05,
          "pricing_required": true
        },
        {
          "item": "Base Cabinet Boxes",
          "unit": "linear_foot",
          "category": "cabinetry", 
          "specification": "Standard base cabinet boxes with adjustable shelves",
          "calculation": "base_cabinet_linear_feet",
          "waste_factor": 0.05,
          "pricing_required": true
        }
      ]
    },
    {
      "category": "doors_and_faces",
      "items": [
        {
          "item": "Cabinet Doors",
          "unit": "each",
          "category": "cabinetry",
          "specification": "Cabinet doors with quality finish",
          "calculation": "door_count_from_linear_feet",
          "waste_factor": 0.02,
          "pricing_required": true
        },
        {
          "item": "Drawer Faces",
          "unit": "each",
          "category": "cabinetry",
          "specification": "Drawer fronts to match cabinet doors",
          "calculation": "drawer_count_from_linear_feet",
          "waste_factor": 0.02,
          "pricing_required": true
        }
      ]
    },
    {
      "category": "hardware",
      "items": [
        {
          "item": "Cabinet Hinges",
          "unit": "each",
          "category": "hardware",
          "specification": "Cabinet door hinges, standard or soft-close",
          "calculation": "door_count * 2",
          "waste_factor": 0.1,
          "pricing_required": true
        },
        {
          "item": "Drawer Slides",
          "unit": "pair",
          "category": "hardware",
          "specification": "Drawer slides, standard or soft-close",
          "calculation": "drawer_count",
          "waste_factor": 0.05,
          "pricing_required": true
        },
        {
          "item": "Pulls and Knobs",
          "unit": "each",
          "category": "hardware",
          "specification": "Cabinet door handles and drawer pulls",
          "calculation": "door_count + drawer_count", 
          "waste_factor": 0.05,
          "pricing_required": true
        }
      ]
    },
    {
      "category": "trim_and_molding",
      "items": [
        {
          "item": "Crown Molding",
          "unit": "linear_foot",
          "category": "trim",
          "specification": "Kitchen crown molding with appropriate profile",
          "calculation": "upper_cabinet_linear_feet",
          "waste_factor": 0.15,
          "condition": "if_crown_molding_specified",
          "pricing_required": true
        },
        {
          "item": "Toe Kick Material",
          "unit": "linear_foot",
          "category": "trim",
          "specification": "Toe kick boards for base cabinets",
          "calculation": "base_cabinet_linear_feet",
          "waste_factor": 0.1,
          "pricing_required": true
        }
      ]
    },
    {
      "category": "installation_supplies",
      "items": [
        {
          "item": "Cabinet Screws 2.5\"",
          "unit": "box_100",
          "category": "fasteners",
          "specification": "Cabinet mounting screws, 2.5 inch length",
          "calculation": "linear_feet / 10",
          "waste_factor": 0,
          "pricing_required": true
        },
        {
          "item": "Shims",
          "unit": "bundle",
          "category": "installation_supplies",
          "specification": "Wood shims for cabinet leveling",
          "calculation": "linear_feet / 8", 
          "waste_factor": 0,
          "pricing_required": true
        },
        {
          "item": "Wood Glue",
          "unit": "bottle",
          "category": "installation_supplies",
          "specification": "Wood adhesive for cabinet installation",
          "calculation": "1_per_job",
          "waste_factor": 0,
          "pricing_required": true
        }
      ]
    }
  ],

  "labor_requirements": {
    "carpenter": {
      "skill_level": "skilled",
      "description": "Experienced cabinet installer",
      "certification_required": false,
      "pricing_required": true
    },
    "helper": {
      "skill_level": "entry_level",
      "description": "General construction helper",
      "certification_required": false,
      "pricing_required": true
    }
  },

  "calculations": {
    "upper_cabinet_linear_feet": {
      "formula": "total_linear_feet * 0.6",
      "description": "Typically 60% of total kitchen perimeter"
    },
    "base_cabinet_linear_feet": {
      "formula": "total_linear_feet * 0.8", 
      "description": "Typically 80% of total kitchen perimeter"
    },
    "door_count_from_linear_feet": {
      "formula": "linear_feet * 1.2",
      "description": "Average 1.2 doors per linear foot"
    },
    "drawer_count_from_linear_feet": {
      "formula": "base_linear_feet * 0.8",
      "description": "Most base cabinets have drawers"
    }
  },

  "complications": [
    {
      "issue": "Walls not plumb or level",
      "likelihood": "high",
      "additional_hours_per_lf": 0.2,
      "additional_material_cost": 25,
      "description": "Requires shimming and adjustments"
    },
    {
      "issue": "Electrical outlet conflicts",
      "likelihood": "medium",
      "additional_hours": 2,
      "additional_material_cost": 50,
      "description": "May need to relocate outlets or modify cabinets"
    },
    {
      "issue": "Plumbing line conflicts", 
      "likelihood": "medium",
      "additional_hours": 3,
      "additional_material_cost": 75,
      "description": "May require cabinet modifications"
    },
    {
      "issue": "Cabinet damage during delivery",
      "likelihood": "low",
      "additional_hours": 0,
      "additional_material_cost": 500,
      "description": "Replacement cabinet needed"
    }
  ],

